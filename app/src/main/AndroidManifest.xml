<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.gndi_sd.szzt">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />

    <application
        android:name="net.soluspay.cashq.utils.MyApplication"
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_logo_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="HardcodedDebugMode">
        <activity
            android:name="net.soluspay.cashq.CardIssuanceActivity"
            android:exported="true" />

        <service
            android:name="net.soluspay.cashq.MyTileService"
            android:icon="@drawable/image_qr"
            android:label="@string/quickpay"
            android:permission="android.permission.BIND_QUICK_SETTINGS_TILE">
            <intent-filter>
                <action android:name="android.service.quicksettings.action.QS_TILE" />
            </intent-filter>
        </service>

        <activity android:name="net.soluspay.cashq.AboutActivity" />
        <activity android:name="net.soluspay.cashq.BillersActivity" />
        <activity android:name="net.soluspay.cashq.ServicePayment" />
        <activity android:name="net.soluspay.cashq.AddCardActivity" />
        <activity android:name="net.soluspay.cashq.SignUpActivity" />
        <activity android:name="net.soluspay.cashq.CardActivity" />
        <activity android:name="net.soluspay.cashq.WorkingKeyActivity" />
        <activity
            android:name="net.soluspay.cashq.SettingsActivity"
            android:label="@string/title_activity_settings" />
        <activity
            android:name="net.soluspay.cashq.ComingSoonActivity"
            android:label="Coming Soon" />
        <activity android:name="net.soluspay.cashq.BalanceInquiryActivity" />
        <activity android:name="net.soluspay.cashq.MOHEArabActivity" />
        <activity android:name="net.soluspay.cashq.MOHEActivity" />
        <activity android:name="net.soluspay.cashq.EducationActivity" />
        <activity android:name="net.soluspay.cashq.PinChangeActivity" />
        <activity android:name="net.soluspay.cashq.CashOutActivity" />
        <activity android:name="net.soluspay.cashq.CashInActivity" />
        <activity android:name="net.soluspay.cashq.VoucherActivity" />
        <activity android:name="net.soluspay.cashq.FundsTransferActivity" />
        <activity android:name="net.soluspay.cashq.TelecomActivity" />
        <activity android:name="net.soluspay.cashq.ElectricityActivity" />
        <activity android:name="net.soluspay.cashq.CardCompletionActivity" />
        <activity
            android:name="net.soluspay.cashq.CustomsActivity"
            android:label="@string/customs_title" />
        <activity
            android:name="net.soluspay.cashq.E15Activity"
            android:label="@string/e15_service" />
        <activity android:name="net.soluspay.cashq.LockActivity" />
        <activity
            android:name="net.soluspay.cashq.MerchantActivity"
            android:label="Merchant Services" />
        <activity android:name="net.soluspay.cashq.BillPaymentActivity" />
        <activity android:name="net.soluspay.cashq.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="web.soluspay.net"
                    android:scheme="https" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="cashq.app"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
            android:name="net.soluspay.cashq.CheckoutActivity"
            android:label="@string/confirm" />
        <activity android:name="net.soluspay.cashq.ResultActivity" />
        <activity
            android:name="net.soluspay.cashq.SignInActivity"
            android:noHistory="true">
            <meta-data
                android:name="android.app.shortcuts"
                android:resource="@xml/shortcuts" />

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="net.soluspay.cashq.PurchaseActivity" />
        <activity android:name="net.soluspay.cashq.QRPayActivity" />
        <activity android:name="net.soluspay.cashq.ScanCodeActivity" />
        <activity android:name="net.soluspay.cashq.IPinActivity" />
        <activity android:name="net.soluspay.cashq.EditCardActivity" />
        <activity android:name="net.soluspay.cashq.QRWebview" />
    </application>

</manifest>